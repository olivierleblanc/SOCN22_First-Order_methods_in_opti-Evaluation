   "source": [
    "import numpy as np\n",
    "\n",
    "M = np.array([[3,1],[1,4]])\n",
    "\n",
    "U, S, _ = np.linalg.svd(M)\n",
    "\n",
    "vals = np.zeros(2)\n",
    "for i in range(2):\n",
    "    vals[i] = np.real(np.roots(np.array([1, -S[i], 0, -1]))[0])\n",
    "\n",
    "res = U@np.diag(vals)@U.T\n",
    "\n",
    "print(res)"
   ]
  }
 ],
